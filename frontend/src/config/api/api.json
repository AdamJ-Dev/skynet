{
  "baseDevUrl": "http://localhost:8081",
  "baseProdUrl": "http://52.56.81.13:8080",
  "flights": {
    "getAirports": {
      "path": "/airports",
      "queryParams": ["search"],
      "queryRegex": "^[A-Za-z0-9\\./:\\-\\(\\)\"']+$"
    },
    "getNearestAirport": {
      "path": "/airport",
      "queryParams": ["lat", "lon"]
    },
    "getFlights": {
      "path": "/flights",
      "queryParams": ["originLocationCode", "destinationLocationCode", "departureDate", "returnDate", "numberOfResults"]
    }
  },
  "weather": {
    "get": {
      "path": "/weather",
      "queryParams": ["lat", "lon", "days"]
    }
  },
  "map": {
    "get": {
      "path": "/map",
      "queryParams": ["lat", "lon"]
    }
  },
  "user": {
    "login": {
      "path": "/authenticate"
    },
    "signup": {
      "path": "/register"
    }
  },
  "epg": {
    "channels": {
      "get": {
        "path": "/epg/channels"
      }
    },
    "programmes": {
      "get": {
        "all": {
          "path": "/epg/programmes"
        },
        "one": {
          "basePath": "/epg/programmes"
        }
      }
    }
  }
}
